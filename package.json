{
	"name": "word-press-blog-by-dojo",
	"version": "1.0.0",
	"scripts": {
		"dev": "dojo build --mode dev --watch --serve",
		"build": "dojo build --mode dist",
		"build:dev": "dojo build --mode dev",
		"prettier": "prettier --write \"src/**/*.{ts,tsx,md,css}\"",
		"test": "dojo test",
		"test:unit": "dojo build --mode unit && dojo test --unit --config local",
		"test:functional": "dojo build --mode functional && dojo test --functional --config local",
		"test:all": "dojo build --mode unit && dojo build --mode functional && dojo test --all --config local"
	},
	"dependencies": {
		"@dojo/framework": "~7.0.0",
		"parse-link-header": "^2.0.0",
		"tslib": "~1.9.1"
	},
	"devDependencies": {
		"@dojo/cli": "~7.0.0",
		"@dojo/cli-build-app": "~7.0.0",
		"@dojo/webpack-contrib": "./dojo-webpack-contrib-7.0.3.tgz",
		"@dojo/cli-test-intern": "~7.0.0",
		"@types/node": "~9.6.5",
		"@types/parse-link-header": "^1.0.0",
		"file-system-cache": "^1.0.5",
		"prettier": "~1.9.0",
		"typescript": "~3.9.0"
	},
	"prettier": {
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true,
		"printWidth": 120,
		"arrowParens": "always"
	}
}
